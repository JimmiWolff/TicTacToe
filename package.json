{
  "name": "tic-tac-toe-multiplayer",
  "version": "1.0.1",
  "description": "Azure-compatible Multiplayer Tic Tac Toe game with login system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "echo 'No build step required'",
    "postinstall": "echo 'Installation complete'",
    "test": "echo 'No tests specified'",
    "android:copy": "cp index.html style.css script.js auth0-spa-js.js www/",
    "android:sync": "npm run android:copy && npx cap sync android",
    "android:open": "npx cap open android"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.4.1",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@sentry/browser": "^10.20.0",
    "@sentry/node": "^10.20.0",
    "@sentry/profiling-node": "^10.20.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "socket.io": "^4.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmiWolff/TicTacToe.git"
  },
  "keywords": [
    "tic-tac-toe",
    "multiplayer",
    "websocket",
    "game",
    "azure",
    "nodejs"
  ],
  "author": "JimmiWolff",
  "license": "MIT",
  "homepage": "https://github.com/JimmiWolff/TicTacToe#readme"
}
